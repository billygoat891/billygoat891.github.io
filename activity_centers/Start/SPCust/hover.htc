<ATTACH EVENT="onmouseover" ONEVENT="hoverMouseOver()"/>
<ATTACH EVENT="onmouseout" ONEVENT="hoverMouseOut()"/>

<PROPERTY NAME="classNormal"/>
<PROPERTY NAME="classHover"/>

<script language="jscript">

function hoverMouseOver()
{
    element.firstChild.className = element.classHover;
}

function hoverMouseOut()
{
    element.firstChild.className = element.classNormal;
}

hoverMouseOut();

</script>
